/dts-v1/;

#include "ast2600-amd-bmc-common.dtsi"

/ {
	model = "AMD Onyx BMC";
	compatible = "amd,onyx-bmc", "aspeed,ast2600";
};

&i2c2 {
	// P0 APML - i2c10
	status = "okay";
	bus-frequency = <400000>;

	sbrmi@3c {
	compatible = "amd,sbrmi";
		reg = <0x3c>;
	};

	sbtsi@4c {
		compatible = "amd,sbtsi";
		reg = <0x4c>;
	};
};
